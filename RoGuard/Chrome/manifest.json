{
  "manifest_version": 3,
  "name": "RoGuard",
  "version": "1.0",
  "description": "Protects you from bad accounts",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.roblox.com/",
    "https://raw.githubusercontent.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.roblox.com/users*"
      ],
      "js": ["main.js"],
      "run_at": "document_idle"
    }
  ],
  "author": "Eagisa"
}